%html
	= render 'layouts/visit/head', favicon: 'emmyicon.ico', frame_stylesheet: 'emmys', frame_js: 'emmys'
	
	%body{ class: content_for( :body_class ), id: content_for( :body_id ) }
		= render 'layouts/visit/header'
		
		#wrapper.container
			= render 'layouts/visit/top'
			= content_for?( :main ) ? yield( :main ) : yield
			= render 'layouts/visit/footer'